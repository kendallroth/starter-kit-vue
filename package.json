    "@vee-validate/yup": "^4.11.7",
    "@vueuse/core": "^10.4.1",
    "assert": "^2.1.0",
    "dayjs": "^1.11.10",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "i18next-vue": "^3.0.0",
    "js-yaml": "^4.1.0",
    "pinia": "^2.1.6",
    "vee-validate": "^4.11.7",
    "vite-plugin-vuetify": "^1.0.2",
