{
  "name": "starter-kit-vue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "api": "sk-api --port 3001",
    "build": "run-p type \"build:only {@}\" --",
    "build:only": "vite build",
    "code:format": "prettier --write \"**/*.{ts,vue}\" --ignore-path .gitignore",
    "code:lint": "eslint \"**/*.{ts,vue}\" --ignore-path .gitignore",
    "code:lint:fix": "npm run code:lint -- --fix",
    "dev": "vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "type": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "@fontsource-variable/manrope": "^5.0.18",
    "@kendallroth/starter-kit-api": "^0.4.0",
    "@mdi/js": "^7.2.96",
    "@tanstack/vue-query": "^4.35.7",
    "@types/js-yaml": "^4.0.9",
    "@vee-validate/yup": "^4.11.7",
    "@vueuse/core": "^10.7.2",
    "assert": "^2.1.0",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "i18next-vue": "^3.0.0",
    "js-yaml": "^4.1.0",
    "msw": "^1.3.2",
    "patch-package": "^8.0.0",
    "pinia": "^2.1.6",
    "vee-validate": "^4.11.7",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.11",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.50.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.0.6",
    "sass": "^1.68.0",
    "type-fest": "^4.3.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.27"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
